/*convert temperature from celcius to farenheit*/
#include <iostream>
using namespace std;
int main(){
    float celsius;
    double farenheight;
    cout<<"Enter Temeperature in Celsius"<<endl;
    cin>>celsius;
    farenheight=((celsius*9)/5)+32;
    cout<<"TEMPERATURE IN FARENHEIGHT    "<<farenheight<<endl;
    return 0;
}
